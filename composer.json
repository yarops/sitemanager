{
	"name": "yarops/sitemanager",
	"description": "Yii 2 Sitemanager",
	"keywords": [
		"yii2"
	],
	"homepage": "http://codesweet.ru/",
	"type": "project",
	"license": "BSD-3-Clause",
	"minimum-stability": "stable",
	"require": {
		"php": ">=8.2",
		"yiisoft/yii2": "~2.0.53",
		"yiisoft/yii2-bootstrap5": "~2.0.4",
		"yiisoft/yii2-symfonymailer": "~2.0.4",
		"kartik-v/yii2-export": "^1.4",
		"kartik-v/yii2-date-range": "^1.7",
		"kartik-v/yii2-widget-depdrop": "^1.0",
		"yiisoft/yii2-queue": "^2",
		"yiisoft/yii2-redis": "^2.0",
		"kartik-v/yii2-bootstrap5-dropdown": "^1.0"
	},
	"require-dev": {
		"yiisoft/yii2-debug": "~2.1.0",
		"yiisoft/yii2-gii": "~2.2.0",
		"yiisoft/yii2-faker": "~2.0.0",
		"phpunit/phpunit": "^9.5",
		"guzzlehttp/guzzle": ">=4.1.4 <7.0",
		"squizlabs/php_codesniffer": "^3.7",
		"yiisoft/yii2-coding-standards": "^3.0"
	},
	"config": {
		"process-timeout": 1800,
		"fxp-asset": {
			"enabled": false
		},
		"allow-plugins": {
			"yiisoft/yii2-composer": true
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	],
	"scripts": {
		"phpcs": "phpcs --standard=ruleset.xml",
		"phpcs-fix": "phpcbf --standard=ruleset.xml",
		"phpcs-check": "phpcs --standard=ruleset.xml --report=summary"
	}
}